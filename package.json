{
  "name": "mida-chemical-inventory",
  "version": "1.0.0",
  "description": "Sistema de Inventarios Químicos - Ministerio de Desarrollo Agropecuario",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "server:dev": "nodemon server/server-dev.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "render-build": "bash render-build.sh"
  },
  "keywords": ["inventory", "chemical", "mida", "quarantine"],
  "author": "MIDA - Dirección Ejecutiva de Cuarentena",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "moment": "^2.29.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
